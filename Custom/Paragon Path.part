    <specific name="Power Name"> Prayers </specific>
    <specific name="Class Features"> ID_FMP_CLASS_FEATURE_829, ID_FMP_CLASS_FEATURE_828, ID_FMP_CLASS_FEATURE_830, ID_FMP_CLASS_FEATURE_831 </specific>
