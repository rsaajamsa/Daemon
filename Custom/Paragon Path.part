    <specific name="Power Name"> Evocations </specific>
    <specific name="Class Features"> ID_FMP_CLASS_FEATURE_3821, ID_FMP_CLASS_FEATURE_3822, ID_FMP_CLASS_FEATURE_3823, ID_FMP_CLASS_FEATURE_3824 </specific>
