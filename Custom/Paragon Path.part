    <specific name="Powers"> ID_FMP_POWER_13532,ID_FMP_POWER_13536,ID_FMP_POWER_13546 </specific>
    <specific name="_DisplayPowers"> ID_FMP_POWER_13532,ID_FMP_POWER_13536,ID_FMP_POWER_13546 </specific>
