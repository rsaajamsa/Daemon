    <print-prereqs> Eladrin </print-prereqs>
    <Flavor> “By the court of the Summer Queen, you shall pay for your transgressions!” </Flavor>
    <specific name="Power Name"> Powers </specific>
    <specific name="Class Features"> ID_FMP_CLASS_FEATURE_1179, ID_FMP_CLASS_FEATURE_1180, ID_FMP_CLASS_FEATURE_1181 </specific>
    <specific name="Powers"> ID_FMP_POWER_5490,ID_FMP_POWER_5491,ID_FMP_POWER_6221 </specific>
