    <specific name="_DisplayPowers"> ID_FMP_POWER_11011,ID_FMP_POWER_11012,ID_FMP_POWER_11014 </specific>
    <rules>
      <grant name="ID_FMP_CLASS_FEATURE_2303" type="Class Feature" Level="11" />
      <grant name="ID_FMP_CLASS_FEATURE_2304" type="Class Feature" Level="11" />
