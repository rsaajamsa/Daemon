  <RulesElement name="Wild Elf" type="Racial Trait" internal-id="ID_FMP_SUBRACE_58" source="Neverwinter Campaign Guide">
    <Category> ID_DRU_RACIAL_TRAIT_004 </Category>
    <specific name="_CS_ShortDescription"> @ </specific>
    <specific name="_PARSED_CHILD_FEATURES"> ID_FMP_RACIAL_TRAIT_3964,ID_FMP_RACIAL_TRAIT_3965 </specific>
