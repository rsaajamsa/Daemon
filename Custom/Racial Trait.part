  <RulesElement name="Moon Elf (Eladrin)" type="Racial Trait" internal-id="ID_FMP_SUBRACE_56" source="Neverwinter Campaign Guide">
    <Category> ID_DRU_RACIAL_TRAIT_002 </Category>
    <specific name="_CS_ShortDescription"> @ </specific>
